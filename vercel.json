{
    "version": 2,
    "builds": [
        {
            "src": "dist/**/*",
            "use": "@vercel/static"
        },
        {
            "src": "assets/**/*",
            "use": "@vercel/static"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/dest/$1"
        }
    ]
}